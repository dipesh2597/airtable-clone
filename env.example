# Environment Variables Template
# Copy this file to .env and fill in your actual values

# Backend Configuration
BACKEND_URL=https://airtable-clone-backend.onrender.com
WS_URL=wss://airtable-clone-backend.onrender.com

# Frontend Environment Variables (for Vercel)
REACT_APP_BACKEND_URL=https://airtable-clone-backend.onrender.com
REACT_APP_WS_URL=wss://airtable-clone-backend.onrender.com

# Optional: Database Configuration (if using external database)
DATABASE_URL=your_database_url_here

# Optional: Security Configuration
SECRET_KEY=your_secret_key_here
CORS_ORIGINS=https://your-frontend-url.vercel.app

# Optional: Logging Configuration
LOG_LEVEL=INFO

# Optional: Authentication (if you add auth later)
# JWT_SECRET=your-secret-key
# GOOGLE_CLIENT_ID=your-google-client-id 